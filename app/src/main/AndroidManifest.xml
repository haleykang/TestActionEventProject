<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="app.android.test.testactioneventproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <!-- 새로운 Activity 클래스 정보를 표현하는 activity 태그 생성 -->

        <activity android:name=".MoveImageActivity">

            <!-- intent-filter 태그 작성 -->
            <intent-filter>

                <!-- action 태그 작성 -->
                <action android:name="android.intent.action.MAIN"/>
                <!-- category 태그 작성 -->
                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".MyTouchButtonActivity">
        </activity>
        <activity android:name=".DatePickUpActivity">
        </activity>
        <activity android:name=".TimePickUpActivity">
        </activity>
    </application>

</manifest>